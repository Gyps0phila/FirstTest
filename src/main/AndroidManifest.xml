<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gypsophila.dailytest" >

    <uses-permission android:name="android.permission.FLASHLIGHT" />
    <uses-permission android:name="android.permission.vibrate" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.gypsophila.dailytest.ViewPagerAty"/>
        <activity android:name="com.example.gypsophila.viewflipper.ViewFlipperAty"/>
        <activity android:name="com.example.gypsophila.scrollview.ScrollViewAty"/>
        <activity android:name="com.example.gypsophila.gallery.GalleryAty"/>
        <activity android:name="com.example.gypsophila.seekbar.SeekBarAty"/>
        <activity android:name="com.example.gypsophila.toast.ToastAty"/>
        <activity android:name="com.example.gypsophila.dialog.DialogAty"/>
        <activity android:name="com.example.gypsophila.notification.NotificationAty"/>
        <activity android:name="com.example.gypsophila.contextmenu.ContextMenuAty"/>
        <activity android:name="com.example.gypsophila.sharedpreferences.SharedPreferencesAty"/>
    </application>

</manifest>
